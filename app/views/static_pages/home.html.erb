<% if logged_in? %>
  <div>
    <%= link_to 'Log Out', logout_path, method: :delete %>
  </div>
  <div>
    <%= link_to "Delete Your Account", current_user, method: :delete, data: {confirm: "Are you sure you want to delete this user?"} %>
  </div>
  <div>
    <%= link_to 'Edit Your Account', edit_user_path(current_user)%>
  </div>
<% else %>  
  <div>
    <%= link_to("Go to Sign up", signup_path) %> or <%= link_to("Log in", login_path) %>
  </div>
<% end %>





